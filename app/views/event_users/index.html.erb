<h1>My Event List</h1>
<div class="container">
  <% @event_users.each do |event_user| %>
  <h1><%= event_user.event.name %></h1>
  <p><%= event_user.event.location %></p>
  <p><%= event_user.event.date_start %></p>
  <%= link_to "Delete", event_user_path(event_user), method: :delete  %>
  <% end %>
</div>
  <%= link_to "Back to the events", categories_events_path %>
